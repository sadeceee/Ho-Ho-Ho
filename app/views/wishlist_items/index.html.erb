<p id="notice"><%= notice %></p>

<h1>Listing wishlist items</h1>
<% @wishlist_items.each do |wishlist_item| %>
<div class="wishlist-box">
<%= link_to wishlist do %>
  <table>
    <thead>
      <tr>
        <th><%= wishlist_item.title %></th>
      </tr>
    </thead>
    <tbody>
      <tr>
        <td><%= wishlist_item.price %></td>
      </tr>
    </tbody>
  </table>

<% end %>
  </div>
<% end %>
<br>

<%= link_to 'New wishlist item', new_wishlist_wishlist_item_path %>
